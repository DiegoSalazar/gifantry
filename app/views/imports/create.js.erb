<% if @importer.success? %>
  $(".modal").modal("hide");
  $("#sidenav").replaceWith("<%= j render('elements/sidenav') %>");
  $("#entries-container").replaceWith('<%= j render("entries/#{@entry_view}") %>');

  <% @importer.entries.each do |entry| %>
    $("#<%= dom_id entry %>").effect("highlight", { color: "#ff8800" }, 3000);
  <% end %>
<% else %>
  $("#modal").find("form").prepend("<%= j render('elements/flash') %>");
<% end %>